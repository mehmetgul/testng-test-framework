<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Regression" >
    <listeners>
        <listener class-name="com.example.htmlreports.extentreports.ExtentReportListener"/>
    </listeners>
    <test name="Regression Test" preserve-order="true">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <packages>
            <package name="com.example.listeners.paralleltesting.pkg1.*"/>
        </packages>

    </test>
</suite>
